/* pages/zhuanji/zhuanji.wxss */
/* 最新专辑 */
.sheet{
    margin-top: 20rpx;
}
.moretitle{
    margin-left: 20rpx;
    font-weight: 700;
    font-size: 32rpx;
}
.sheetbox{
    float: left;
    width: 30%;
    /* border: 1px solid #ccc; */
    height: 350rpx;
    margin: 12rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
}
.sheet_img{
    width: 100%;
    height: 220rpx;
    border-radius: 15rpx;
}
.sheet_text{
    display: block;
    font-size: 24rpx;
    line-height: 24rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    margin-bottom: 20rpx;
    margin-top: 10rpx;
}
.com{
    color: #ccc;
}